{
  "name": "pti-gympal-doc",
  "version": "1.0.0",
  "description": "Complete technical documentation for GymPal project - Social fitness platform with integrated artificial intelligence",
  "main": "index.js",
  "scripts": {
    "dev": "python -m http.server 8000",
    "serve": "python -m http.server 8000",
    "build": "echo 'Documentation is static Markdown files'",
    "test": "echo 'No tests for documentation'",
    "lint": "markdownlint docs/**/*.md",
    "lint:fix": "markdownlint --fix docs/**/*.md",
    "validate": "markdownlint docs/**/*.md && echo 'All markdown files are valid'",
    "deploy": "echo 'Deploy to GitHub Pages via GitHub Actions'",
    "docker:build": "docker build -t gympal-docs .",
    "docker:run": "docker run -d -p 3000:3000 --name gympal-docs gympal-docs",
    "docker:stop": "docker stop gympal-docs && docker rm gympal-docs",
    "docker:logs": "docker logs -f gympal-docs",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs:compose": "docker-compose logs -f"
  },
  "packageManager": "pnpm@8.15.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/PTI-GymPalDoc.git"
  },
  "keywords": [
    "gympal",
    "fitness",
    "social",
    "ai",
    "documentation",
    "nextjs",
    "hono",
    "supabase",
    "typescript",
    "kubernetes",
    "devops"
  ],
  "author": "GymPal Team FIB-UPC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/username/PTI-GymPalDoc/issues"
  },
  "homepage": "https://username.github.io/PTI-GymPalDoc",
  "devDependencies": {
    "markdownlint-cli": "^0.37.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
